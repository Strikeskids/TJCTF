mixin htmlbase(title)
	doctype
	html(lang='en')
		head
			title= title
			link(rel='stylesheet',type='text/css',href='/css/main.css')
		body
			header
				div.logo TJCSC
				nav
					ul.navigation
						li: a(href='/') Home
						li: a Wiki
						li: a(href='/problems') Problems
						li: a Scores
						li: a About
				div.user
					if user && typeof user !== 'undefined'
						a= user.username
						|  | 
						a(href='/logout') Logout
					else
						a(href='/login') Log in
						|  | 
						a(href='/register') Register
			if block
				block